<?xml version="1.0" encoding="utf-8"?>
<layer-list
  xmlns:android="http://schemas.android.com/apk/res/android">
    <item android:id="@android:id/background">
        <shape>
            <size android:height="@dimen/margin_9dp" />
            <corners android:radius="6.0dip" />
            <gradient android:startColor="#339c9ca5" android:endColor="#339c9ca5" android:angle="0.0" />
        </shape>
    </item>
    <item android:id="@android:id/secondaryProgress">
        <scale android:scaleWidth="100%">
            <shape>
                <corners android:radius="20.0dip" />
                <solid android:color="@color/transparent" />
            </shape>
        </scale>
    </item>
    <item android:id="@android:id/progress">
        <scale android:drawable="@drawable/evaluate_progress_first_bg" android:scaleWidth="100%" />
    </item>
</layer-list>
