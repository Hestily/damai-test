<?xml version="1.0" encoding="utf-8"?>
<layer-list
  xmlns:android="http://schemas.android.com/apk/res/android">
    <item android:id="@android:id/progress">
        <rotate android:fromDegrees="0.0" android:toDegrees="360.0" android:pivotX="50.0%" android:pivotY="50.0%">
            <shape android:shape="ring" android:innerRadiusRatio="3.0" android:thicknessRatio="20.0" android:thickness="4.0dip">
                <solid android:color="@color/rpsdk_loading_bg" />
            </shape>
        </rotate>
    </item>
</layer-list>
