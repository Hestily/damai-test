<?xml version="1.0" encoding="utf-8"?>
<selector
  xmlns:android="http://schemas.android.com/apk/res/android">
    <item android:state_selected="true">
        <shape android:shape="rectangle">
            <solid android:color="@color/white" />
            <corners android:topLeftRadius="13.0dip" android:topRightRadius="13.0dip" android:bottomLeftRadius="2.0dip" android:bottomRightRadius="13.0dip" />
            <stroke android:width="2.0dip" android:color="#ffc6c6c6" />
        </shape>
    </item>
    <item android:state_selected="false">
        <shape android:shape="rectangle">
            <corners android:topLeftRadius="13.0dip" android:topRightRadius="13.0dip" android:bottomLeftRadius="2.0dip" android:bottomRightRadius="13.0dip" />
            <gradient android:startColor="#ffffbeed" android:endColor="#ffff70b8" android:angle="270.0" />
        </shape>
    </item>
</selector>
